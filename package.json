{
  "name": "usermanagement-service",
  "version": "0.3.0",
  "description": "Simple User Management Service",
  "main": "src/index.js",
  "scripts": {
    "test": "failsafe clean test:update-serenity build:test test:acceptance test:report",
    "clean": "rimraf target reports",
    "test:update-serenity": "serenity-bdd update",
    "test:acceptance": "failsafe test:backend:mock test:rest:mock",
    "test:backend:mock": "cucumber-js --profile backendMock",
    "test:rest:mock": "cucumber-js --profile restMock",
    "test:report": "failsafe test:report:backend:mock test:report:rest:mock",
    "test:report:backend:mock": "serenity-bdd run --source target/backendMock --destination reports/backendMock --project Usermanagement-Service",
    "test:report:rest:mock": "serenity-bdd run --source target/restMock --destination reports/restMock --project Usermanagement-Service",
    "verify": "npm test",
    "build": "tsc --project ./tsconfig.json",
    "build:watch": "tsc --watch --project ./tsconfig.json",
    "build:test": "tsc --project ./features/stepdefs/tsconfig.json",
    "build:test:watch": "tsc --watch --project ./features/stepdefs/tsconfig.json",
    "lint": "eslint ."
  },
  "author": "Tobias Klauenberg",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0",
    "@serenity-js/assertions": "^2.24.1",
    "@serenity-js/console-reporter": "^2.24.1",
    "@serenity-js/core": "^2.24.1",
    "@serenity-js/cucumber": "^2.24.1",
    "@serenity-js/local-server": "^2.24.1",
    "@serenity-js/rest": "^2.24.1",
    "@serenity-js/serenity-bdd": "^2.24.1",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.32",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.3",
    "npm-failsafe": "^0.4.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
